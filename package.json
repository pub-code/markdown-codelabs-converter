{
  "name": "markdown-codelabs-converter",
  "version": "1.0.0",
  "description": "Convert Markdown files to Google Codelabs style web pages",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "d1:create": "wrangler d1 create codelabs-db",
    "d1:init": "wrangler d1 execute codelabs-db --local --file=./schema.sql",
    "d1:clear:local": "wrangler d1 execute codelabs-db --local --file=./clear.sql",
    "d1:init:remote": "wrangler d1 execute codelabs-db --remote --file=./schema.sql",
    "d1:clear:remote": "wrangler d1 execute codelabs-db --remote --file=./clear.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "markdown",
    "codelabs",
    "converter",
    "tutorial",
    "documentation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.0.0",
    "marked": "^9.1.2",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
